<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean id="pessoa"
        class="br.gov.sp.fatec.GerarPessoa">
        <property name="dados" ref="mensagem"/>
    </bean>
    
    <bean id="mensagem"
        class="br.gov.sp.fatec.Pessoa">
        <property name="nome" value="Carlos"/>
        <property name="sobrenome" value="neto"/>
        <property name="idade" value="50"/>
        <property name="telefone" value="129999999"/>
    </bean>
</beans>
